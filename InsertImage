package JavaSwing;
import java.awt.*;
import java.swing.JFrame;

public class imagescanvas extends Canvas{
 public void paint(Graphics g)
 {
 Toolkit t=Toolkit.getDefaultToolkit();
 Image i=t.getImage("C:\\Users\\ABC\\....png");
 g.drawImage(i,120,120,this);
 }
 public static void main (String args[])
 {
  Imagescanvas m=new imagecanvas();
  JFrame f=new JFrame();
  f.add(m);
  f.setSize(400,400);
  f.setVisible(true);
  }}
 
 
